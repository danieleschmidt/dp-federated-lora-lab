{
  "experiment_name": "quickstart_demo",
  "num_clients": 5,
  "num_rounds": 3,
  "model_name": "distilbert-base-uncased",
  "privacy": {
    "epsilon": 8.0,
    "delta": 1e-05,
    "noise_multiplier": 1.1
